<button class="navbar-toggler sidebarButton" type="button" data-toggle="collapse" data-target="#left-nav-sidebar" aria-controls="left-nav-sidebar" aria-expanded="false" aria-label="Toggle navigation">
    {{PageName}} <span class="carrot-down">&#9660;</span> 
    </button>
<ul class="sidebar-left collapse navbar-collapse" id="left-nav-sidebar">

  <li class="sideL nav-item dropdown">
    <a class="nav-link" data-toggle="collapse" href="#collapseVersions" aria-expanded="false"
      aria-controls="collapseVersions">
      Releases <span class="carrot-down">&#9660;</span>
    </a>
    <div class="collapse" id="collapseVersions">
      <a class="nav-link sidebar-subHeading {{#ifEquals '0.3.0' Version}}active{{/ifEquals}}" href="#">
        <span>
          0.3.0
        </span>
      </a>
      <a class="nav-link sidebar-subHeading {{#ifEquals '0.2.0' Version}}active{{/ifEquals}}" href="#">
        <span>
          0.2.0
        </span>
      </a>
      <a class="nav-link sidebar-subHeading {{#ifEquals '0.1.0' Version}}active{{/ifEquals}}" href="#">
        <span>
          0.1.0
        </span>
      </a>
    </div>
  </li>

  <li class="sideL nav-item">
    <a class="nav-link {{#ifEquals 'Overview' PageName}}active{{/ifEquals}}" href="{{lang}}/docs/overview.html">
      <span>
        Overview
      </span>
    </a>
  </li>
  <li class="sideL nav-item">
    <a class="nav-link {{#ifEquals 'Tutorials' PageName}}active{{/ifEquals}}" href="{{lang}}/docs/tutorials.html">
      <span>
        Tutorials
      </span>
    </a>
  </li>
  <li class="sideL nav-item">
    <a class="nav-link {{#ifEquals 'Components' PageName}}active{{/ifEquals}}" href="#">
      <span>
        Components
      </span>
    </a>
  </li>
  {{#each AllComponents}}
  <li class="sideL nav-item">
    <a class="nav-link sidebar-subHeading {{#ifEquals this.[0] ../PageName}}active{{/ifEquals}}"
      href="{{../lang}}/docs/components/{{this.[1]}}.html">
      <span>
        {{this.[0]}} 
      </span>
    </a>
  </li>
  {{/each}}
</ul>